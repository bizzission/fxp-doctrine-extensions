{
    "name": "sonatra/doctrine-extensions",
    "type": "library",
    "description": "Add doctrine unique entity validator",
    "keywords": ["doctrine", "extensions", "validator", "unique entity"],
    "homepage": "https://github.com/sonatra/sonatra-doctrine-extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "doctrine/orm": "^2.6.0"
    },
    "require-dev": {
        "symfony/http-kernel": "^3.4.0|^4.0.0",
        "symfony/event-dispatcher": "^3.4.0|^4.0.0",
        "symfony/doctrine-bridge": "^3.4.0|^4.0.0",
        "symfony/validator": "^3.4.0|^4.0.0"
    },
    "suggest": {
        "stof/doctrine-extensions-bundle": "For use some nice extensions for doctrine",
        "creof/doctrine2-spatial": "For add geo spacial extensions"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Component\\DoctrineExtensions\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
