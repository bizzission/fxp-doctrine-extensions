{
    "name": "fxp/doctrine-extensions",
    "type": "library",
    "description": "Add doctrine unique entity validator",
    "keywords": ["doctrine", "extensions", "validator", "unique entity"],
    "homepage": "https://github.com/fxpio/fxp-doctrine-extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "fxp/doctrine-extra": "^2.0.0",
        "doctrine/persistence": "^1.3",
        "doctrine/orm": "^2.7"
    },
    "require-dev": {
        "symfony/http-kernel": "^5.0.0",
        "symfony/event-dispatcher": "^5.0.0",
        "symfony/translation": "^5.0.0",
        "doctrine/annotations": "^1.10",
        "doctrine/cache": "^1.10",
        "doctrine/event-manager": "^1.1",
        "doctrine/reflection": "^1.2",
        "symfony/doctrine-bridge": "^5.0.0",
        "symfony/validator": "^5.0.0"
    },
    "suggest": {
        "stof/doctrine-extensions-bundle": "For use some nice extensions for doctrine",
        "creof/doctrine2-spatial": "For add geo spacial extensions"
    },
    "autoload": {
        "psr-4": { "Fxp\\Component\\DoctrineExtensions\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
